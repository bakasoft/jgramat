value = string | object;

string = @value('a-z');

entry = @set(@name(string) ":" value);

object-impl = @object("{" { entry / "," } "}");

object = object-impl;

@pass<"a">(value);
@pass<"{a:b}">(value);
@pass<"{a:{}}">(value);
@pass<"{a:b,c:d}">(value);
@pass<"{a:{},b:{}}">(value);
@pass<"{a:{b:c},d:{e:f}}">(value);
