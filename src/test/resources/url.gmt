Entry := <name: {1 !("=" | "&" | "#") }> [ "=" <value: { !("&" | "#") }> ]

Url :=
    [ <scheme: {1 !":" }> ":" ]
    [ "//" [ <userinfo: { !"@" }> "@" ] <host: {1 !(":" | "/") }> [ ":" <port: {1 !"/" }> ] ]
    <path: {1 !"?" }>
    [ "?" { <query +: Entry> / {1 "&" } } { "&" } ]
    [ "#" <fragment: { !$ }>] $