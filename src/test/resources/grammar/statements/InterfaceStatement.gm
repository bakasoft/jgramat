@object InterfaceStatement =
    [ @set(`accessModifier`: AccessModifier) ws ]
    "interface" ws @set(`name`: Name)
    [ ws @set(`genericParameters`: GenericParameterGroup) ]
    [ ws "implements" ws @set(`superInterfaces`: TypeRefList) ]
    [ ws @set(`members`: InterfaceMemberList) ]

@list InterfaceMemberList = "{" { ws InterfaceMember } ws "}"

InterfaceMember =
    InterfaceField |
    InterfaceFunction |
    InterfaceOperation |
    InterfaceConstructor

@object InterfaceField =
    [ @set(`accessModifier`: AccessModifier) ws ]
    @set(`fieldType`: FieldType) ws @set(`name`: Name)
    [ ws @set(`dataType`: TypeRef) ]

@object InterfaceFunction =
    [ @set(`accessModifier`: AccessModifier) ws ]
    "function" ws @set(`name`: Name)
    ws @set(`parameters`: ParameterGroup)
    [ i-ws @set(`returnType`: TypeRef) ]

@object InterfaceOperation =
    [ @set(`accessModifier`: AccessModifier) ws ]
    "operation" ws @set(`name`: Name) ws @set(`parameters`: ParameterGroup)
    [ i-ws @set(`returnType`: TypeRef) ]

@object InterfaceConstructor =
    [ @set(`accessModifier`: AccessModifier) ws ]
    "constructor" ws @set(`name`: Name) ws @set(`parameters`: ParameterGroup)
