Alpha = 'a-z A-Z _';
Digit = '0-9';

Value = ws (String | Number | Boolean | Null | Array | Object) ws;

String = @value("\"" {'a-z'} "\"");

Number = @value(['+ -'] {+ Digit });

True = @value("true");
False = @value("false");
Boolean = True | False;

Null = @value("null");

Array = @array("[" { Value / "," } ws "]");

Object = @object("{" { ws @set(@name(String) ws ":" Value) / "," } ws "}");

ws = {" " | '\t \n \r'};

#@pass<"\"\"">(String);

#@pass<"0">(Number);
#@pass<"10">(Number);
#@pass<"+100">(Number);
#@pass<"-200">(Number);

#@pass<"true">(Boolean);
#@pass<"false">(Boolean);

#@pass<"null">(Null);

#@pass<"[]">(Value);
@pass<"[1, 2]">(Value);
@pass<"{\"a\": null}">(Value);
@pass<"{\"a\": [1, {}]}">(Value);
