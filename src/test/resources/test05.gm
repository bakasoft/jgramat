Alpha = 'a-z A-Z _';
Digit = '0-9';

Value = ws (String | Number | Boolean | Null | Array | Object) ws;

String = @value(Alpha { Alpha | Digit });

Number = @value(['+ -'] {+ Digit });

True = @value("true");
False = @value("false");
Boolean = True | False;

Null = @value("null");

Array = "[" { Value / "," } ws "]";

Object = @object("{" { ws @set(@name(String) ws ":" Value) / "," } ws "}");

ws = {" " | '\t \n \r'};

#@pass<"a">(String);
#@pass<"ab">(String);
#@pass<"ab_">(String);
#@pass<"ab_x">(String);

#@pass<"0">(Number);
#@pass<"10">(Number);
#@pass<"+100">(Number);
#@pass<"-200">(Number);

#@pass<"true">(Boolean);
#@pass<"false">(Boolean);

#@pass<"null">(Null);

@pass<"[]">(Array);