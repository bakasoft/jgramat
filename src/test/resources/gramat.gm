Test-01 = ^ @object("{" { @set(@string({+'a-z'}): ":" @string({+'a-z'})) / "," } "}") $

@match Test-01 `{abc:def,ghi:jkl,mno:pqr}` {abc:def, ghi:jkl, mno: pqr}

Test-11 = ^ "{" { {+"a"} ":" {+"b"} / "," } "}" $

@pass Test-11 `{aaa:bbb,aaa:bbb,aaa:bbb,aaa:bbb}`
@pass Test-11 `{aa:bb,aa:bb,aa:bb,aa:bb}`
@pass Test-11 `{a:b,a:b,a:b,a:b}`
@pass Test-11 `{aaa:bbb,aaa:bbb,aaa:bbb}`
@pass Test-11 `{aa:bb,aa:bb,aa:bb}`
@pass Test-11 `{a:b,a:b,a:b}`
@pass Test-11 `{aaa:bbb,aaa:bbb}`
@pass Test-11 `{aa:bb,aa:bb}`
@pass Test-11 `{a:b,a:b}`
@pass Test-11 `{aaa:bbb}`
@pass Test-11 `{aa:bb}`
@pass Test-11 `{a:b}`
@pass Test-11 `{}`

Test-00 = ^ {"ab"} {"cd" / "|"} {+"ef"} {+"gh" / "|"} $

@fail Test-00 ``
@fail Test-00 `ab`
@fail Test-00 `cd`
@fail Test-00 `ef`
@fail Test-00 `gh`
@fail Test-00 `cdef`
@pass Test-00 `efgh`
@pass Test-00 `cdefgh`
@pass Test-00 `abcdefgh`
@pass Test-00 `abefgh`
@pass Test-00 `ababcdefgh|gh`
@pass Test-00 `cd|cdefefgh|gh`
@pass Test-00 `ababcd|cdefefgh|gh`

Test-06 = ^ "/*" ("\\*/"|*) "*/" $

@pass Test-06 `/**/`
@fail Test-06 ``
@pass Test-06 `/*a*/`
@pass Test-06 `/*aa*/`
@pass Test-06 `/*a*a*/`
@pass Test-06 `/*a\\/*a*/`

Test-04 = ^ ("0" | '1-9' { '0-9' }) $

@fail Test-04 `01`
@pass Test-04 `0`
@pass Test-04 `1`
@pass Test-04 `12`
@pass Test-04 `123`

Test-02 = ^ @object("=" {" "} @set(`id`: @string('0-9' {'0-9'})) {" "} ";") $

@match Test-02 `= 1234;` {id: `1234`}

Test-03 = ^ "a" ["_"] "b" $

@fail Test-03 ``
@fail Test-03 `a`
@fail Test-03 `a_`
@pass Test-03 `ab`
@pass Test-03 `a_b`

Test-05 = ^ ("abc"|"xyz"|"123") $

@fail Test-05 ``
@fail Test-05 `a`
@fail Test-05 `ab`
@pass Test-05 `abc`
@pass Test-05 `xyz`
@pass Test-05 `123`

Test-07 = ^ {"ab"} "ac" $

@pass Test-07 `ac`
@pass Test-07 `abac`
@pass Test-07 `ababac`
@pass Test-07 `abababac`

Test-08 = ^ "'" * "'" $

@fail Test-08 ``
@fail Test-08 `'`
@pass Test-08 `''`
@fail Test-08 `'''`
@pass Test-08 `'a'`
@pass Test-08 `'ab'`
@pass Test-08 `'abc'`
@pass Test-08 `'abcd'`

Test-09 = ^ "a" * "e" $

@fail Test-09 ``
@fail Test-09 `a`
@pass Test-09 `ae`
@fail Test-09 `ab`
@fail Test-09 `ax`
@fail Test-09 `abc`
@fail Test-09 `axx`
@fail Test-09 `abcd`
@fail Test-09 `axxx`
@pass Test-09 `abcde`
@pass Test-09 `abcxe`
@pass Test-09 `abxxe`
@pass Test-09 `axxxe`
@fail Test-09 `axxxx`
@pass Test-09 `axxxxe`

Test-10 = ^ "a" {"b"} "c" $

@fail Test-10 ``
@fail Test-10 `a`
@pass Test-10 `ac`
@pass Test-10 `abc`
@pass Test-10 `abbc`
@pass Test-10 `abbbc`

Test-12 = ^ @object("{" { @set(@string({+'a-z'}): ":" @string({+'a-z'})) / "," } "}") $

@match Test-12 `{abc:def,ghi:jkl,mno:pqr}` {abc:def, ghi:jkl, mno: pqr}