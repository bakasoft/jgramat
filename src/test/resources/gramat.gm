Test-03 = ^ "a" ["_"] "b" $

@fail Test-03 ``
@fail Test-03 `a`
@fail Test-03 `a_`
@pass Test-03 `ab`
@pass Test-03 `a_b`

Test-04 = ^ ("0" | '1-9' { '0-9' }) $

@fail Test-04 `01`
@pass Test-04 `0`
@pass Test-04 `1`
@pass Test-04 `12`
@pass Test-04 `123`

Test-05 = ^ ("abc"|"xyz"|"123") $

@fail Test-05 ``
@fail Test-05 `a`
@fail Test-05 `ab`
@pass Test-05 `abc`
@pass Test-05 `xyz`
@pass Test-05 `123`

Test-06 = ^ "/*" ("\\*/"|*) "*/" $

@fail Test-06 ``
@pass Test-06 `/**/`
@pass Test-06 `/*a*/`
@pass Test-06 `/*aa*/`
@pass Test-06 `/*a*a*/`
@pass Test-06 `/*a\\/*a*/`

