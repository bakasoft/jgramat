X = @string("000") | @integer("001")

TestX = ^ X $

@match TestX `001` `1`
@match TestX `000` `000`

@string String = {+'a-z'}

TestString = ^ String $

@match TestString `a` a
@match TestString `ab` ab
@match TestString `abc` abc

@object Object = "{" { @set(String: ":" (String | Object)) / "," } "}"

TestObject = ^ Object $

@match TestObject `{}` {}
@match TestObject `{a:{}}` {a:{}}