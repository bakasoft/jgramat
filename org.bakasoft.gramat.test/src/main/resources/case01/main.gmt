/* Simple URL parser */

entry = name: <.*> ("=" value: <.*>)?;
url = baseUrl: <.*> path: <"/" .*>
    ( "?" query +: {entry}? ("&" query +: {entry})* "&"? )?
    ( "#" fragment: <.*>)?;
    
